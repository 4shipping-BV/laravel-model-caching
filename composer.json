    },
    "require-dev": {
        "laravel/laravel": "5.5.*",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "5.7.*",
        "satooshi/php-coveralls" : "dev-master@dev",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\": "src/",
            "GeneaLabs\\LaravelModelCaching\\Tests\\": "tests/"
        }
    }
}
